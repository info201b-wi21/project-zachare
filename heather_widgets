  title = "Health Expenditures over Time",
  sidebarLayout(
    sidebarPanel(
      sliderInput(
        inputId = "year",
        label = "Year",
        min = 2011,
        max = 2018,
        value = c(2011, 2018)),
      selectInput(
        inputId = "Country.Name",
        label = "Country",
        c(unique(as.character(country_trend_df$Country.Name)))),
      textOutput(
        outputId = "my_message"
      )
